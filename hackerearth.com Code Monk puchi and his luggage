int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n,i,j;
        scanf("%d",&n);
        int a[n];
        for(i=0;i<n;++i)
            scanf("%d",&a[i]);
        for(j=0;j<n;++j)
        {
            int count=0;
            for(i=j+1;i<n;++i)
            {
                if(a[i]<a[j])
                count++;
            }
            printf("%d ",count);
        }
    }
}
